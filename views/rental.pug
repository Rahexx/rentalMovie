nav#colorlib-main-nav(role='navigation')
  a.js-colorlib-nav-toggle.colorlib-nav-toggle.active(href='#')
    i
  .js-fullheight.colorlib-table
    .colorlib-table-cell.js-fullheight
      .row
        .col-md-12.text-center
          ul
            li
              a(href='/')
                span Strona glowna
            li.active
              a(href='/rental')
                span Wypozyczenia
            li
              a(href='/movie')
                span Filmy
            li
              a(href='/logout')
                span Wyloguj 
#colorlib-page
  header
    .container
      .row
        .col-md-12
          a.js-colorlib-nav-toggle.colorlib-nav-toggle(href='#')
            i
  section.home-slider.owl-carousel
    .slider-item(style="background-image: url('images/bg_3.jpg');" data-stellar-background-ratio='0.5')
      .overlay
      .container(data-scrollax-parent='true')
        .row.slider-text.align-items-end
          .col-md-7.col-sm-12.ftco-animate.mb-5
            h1.mb-3(data-scrollax=" properties: { translateY: '70%', opacity: .9}") Wypo&zdot;yczenia
  section.ftco-section.bg-light
    .container
      .row.justify-content-center.mb-5.pb-5
        .col-md-7.text-center.heading-section.ftco-animate
          h2 Aktualne wypozyczenia
      .row
        .col-md-12.mb-5
          .classes-wrap.d-flex.pb-4.mb-4.ftco-animate
            .one-half.d-md-flex
              div
                span.time Data
              .px-md-5
                h3 Tytu&lstrok;
                p Opis filmu
  section.ftco-section
    .container
      .row.justify-content-center.mb-5.pb-5
        .col-md-7.text-center.heading-section.ftco-animate
          h2 Historia wypozycze&nacute;
      .row
        .col-md-12.mb-5
          .classes-wrap.d-flex.pb-4.mb-4.ftco-animate
            .one-half.d-md-flex
              div
                span.time Data
              .px-md-5
                h3 Tytu&lstrok;
                p Opis filmu
  footer.ftco-footer.ftco-bg-dark.ftco-section
    .container
      .row.mb-5.justify-content-center
        .col-md-5.text-center
          .ftco-footer-widget.mb-5
            h4.mb-4 Madev
            ul.ftco-footer-social.list-unstyled
              li.ftco-animate
                a(href='#')
                  span.icon-twitter
              li.ftco-animate
                a(href='#')
                  span.icon-facebook
              li.ftco-animate
                a(href='#')
                  span.icon-instagram
          .ftco-footer-widget
            h2.mb-3 Contact Us
            p.h3.email
              a(href='#') Madev@info.com
      .row
        .col-md-12.text-center
          p
